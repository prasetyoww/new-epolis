<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EPolis</title>
    <link href="~/template/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/template/css/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/template/css/_all-skins.min.css" rel="stylesheet" />
    <link href="~/template/css/datatables.min.css" rel="stylesheet" />
    @*<link href="https://cdn.datatables.net/v/dt/dt-1.10.15/datatables.min.css" rel="stylesheet" />*@
    <link href="~/template/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/template/css/ionicons.min.css" rel="stylesheet" />
    <link href="~/template/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/template/css/select2.min.css" rel="stylesheet" />
    <link href="~/css/Table.css" rel="stylesheet" />
    <link href="~/css/skinbni.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/limonte-sweetalert2/10.5.1/sweetalert2.min.css" rel="stylesheet" />
    <link rel="icon" type="image/png" href="~/img/logo46.jpeg" />
</head>
<body class="hold-transition skin-bni sidebar-mini">
    <div class="wrapper">

        <header class="main-header">
            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>B</b>NI</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg">
                    <img src="~/img/logobni.png" />
                </span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>

                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        @*<li><a asp-area="" asp-controller="Account" asp-action="Logout">Sign Out</a></li>*@
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <asp:Label ID="lblname" runat="server" Text="Anis"></asp:Label>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="user-header">
                                    @*<p>
                                            <asp:Label ID="lblusername" runat="server" Text="Label"></asp:Label> -
                                        </p>*@
                                </li>
                                <li class="user-footer">
                                    <div class="pull-right">

                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        @*<body>
            <header>
                <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
                    <div class="container">
                        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">EPolis</a>
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                                aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>
                        <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                            <ul class="navbar-nav flex-grow-1">
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </nav>
            </header>*@

        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        @*<img src="images/task-icon.jpg" class="img-circle" alt="User Image" />*@
                    </div>
                    <div class="pull-left info">
                        <p><asp:Label ID="lblname2" runat="server" Text="Label"></asp:Label></p>
                        <a href="#"><i class="fa fa-circle text-success"></i>Online</a>
                    </div>
                </div>
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">MAIN MENU</li>

                    <li class="treeview">
                        <a>
                            <i class="fa fa-file"></i>
                            <span>MASTER DATA</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li class="treeview">
                                <a href="#">
                                    <i class="fa fa-circle-o"></i> Penutupan
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a class="nav-link text-dark" asp-area="" asp-controller="FormPenutupans" asp-action="Index">Input Penutupan</a>
                                    <li><a class="nav-link text-dark" asp-area="" asp-controller="Penutupans" asp-action="Index">List Penutupan</a>
                                    <li><a class="nav-link text-dark" asp-area="" asp-controller="" asp-action="Index">Approval</a>
                                    <li class="treeview">
                                        <a href="#">
                                            <i class="fa fa-circle-o"></i> Parameter
                                            <span class="pull-right-container">
                                                <i class="fa fa-angle-left pull-right"></i>
                                            </span>
                                        </a>
                                        <ul class="treeview-menu">
                                            <li><a class="nav-link text-dark" asp-area="" asp-controller="JenisAsuransi" asp-action="Index">Daftar Jenis Asuransi</a>
                                            <li><a class="nav-link text-dark" asp-area="" asp-controller="PerusahaanAsuransis" asp-action="Index">Daftar Perusahaan Asuransi</a>
                                            <li> <a class="nav-link text-dark" asp-area="" asp-controller="Okupasi" asp-action="Index">Daftar Okupasi</a>
                                            <li> <a class="nav-link text-dark" asp-area="" asp-controller="JenisPertanggungan" asp-action="Index">Daftar Jenis Pertanggungan</a>
                                            <li> <a class="nav-link text-dark" asp-area="" asp-controller="Broker" asp-action="Index">Daftar Broker</a>
                                            <li> <a class="nav-link text-dark" asp-area="" asp-controller="Perluasan" asp-action="Index">Daftar Perluasan</a>
                                            <li> <a class="nav-link text-dark" asp-area="" asp-controller="Pertanggungan" asp-action="Index">Daftar Pertanggungan</a>
                                        </ul>
                                    </li>

                                </ul>
</ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper" style="height:auto">
            <!-- Content Header (Page header) -->
            <section class="content container-fluid">
                <div class="container body-content">
                    <div class="box">
                        <div class="box-header with-border">
                            <div class="box-body">
                                @RenderBody()
                                <hr />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="control-sidebar-bg"></div>
            </section>
        </div>

        @*<div class="container">
                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
            </div>*@
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                @*<b>Version</b> 5.8.19*@

            </div>

        </footer>
        <div class="control-sidebar-bg"></div>
    </div>

    @*<footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2020 - EPolis - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer>*@
    <script src="~/template/js/jquery.min.js" asp-append-version="true"></script>
    <script src="~/template/js/bootstrap.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/jquery-ui.min.js" asp-append-version="true"></script>
    <script src="~/js/site.min.js" asp-append-version="true"></script>
    <script src="~/template/js/adminlte.min.js" asp-append-version="true"></script>
    <script src="~/template/js/datatables.min.js" asp-append-version="true"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/limonte-sweetalert2/10.5.1/sweetalert2.min.js~/template/js/datatables.min.js" asp-append-version="true"></script>
    <script src="~/js/DataTablescript.js"></script>


    @*<script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>*@
    @RenderSection("Scripts", required: false)
</body>
</html>
